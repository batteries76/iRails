<p id="notice"><%= notice %></p>

<h1>Artists Comments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists_comments.each do |artists_comment| %>
      <tr>
        <td><%= link_to 'Show', artists_comment %></td>
        <td><%= link_to 'Edit', edit_artists_comment_path(artists_comment) %></td>
        <td><%= link_to 'Destroy', artists_comment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artists Comment', new_artists_comment_path %>
