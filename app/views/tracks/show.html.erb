<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @track.title %>
</p>

<p>
  <strong>Length:</strong>
  <%= @track.length %>
</p>

<p>
  <strong>Albums:</strong>
  <ul>
    <% @track.albums.each do |album| %>
      <li>
        <strong><%= album.title %></strong> by <%= album.artist.name %>
      </li>
    <% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_track_path(@track) %> |
<%= link_to 'Back', tracks_path %>
